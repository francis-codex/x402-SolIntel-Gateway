{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dashboard/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/gateway/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/gateway/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dashboard/$1"
    }
  ],
  "env": {
    "NODE_VERSION": "20"
  }
}
